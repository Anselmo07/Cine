{
    "builds": [
      {
        "src": "dist/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/HistoriaCine", "dest": "/HistoriaCine.html" },
      { "src": "/formulario", "dest": "/formulario.html" },
      { "src": "/About", "dest": "/About.html" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  